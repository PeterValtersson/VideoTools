{
  "manifest_version": 3,
  "name": "CBD",
  "description": "Stream/video downloader",
  "version": "1.0",
  "action": {
    "default_popup": "menu.html",
    "default_icon": "download.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "host_permissions": [
    "http://localhost:5000/*"
  ],
  "permissions": [ 
   "activeTab",
   "clipboardRead"
  ]
}